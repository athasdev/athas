      "py": "python",
      "rs": "rust",
      "go": "go",
      "java": "java",
      "yaml": "yaml",
      "yml": "yaml",
      "toml": "toml",
      "png": "image",
      "jpg": "image",
      "jpeg": "image",
      "gif": "image",
      "svg": "image",
      "webp": "image"
    },
    "fileNames": {
      ".gitignore": "git",
      ".gitattributes": "git",
      ".gitmodules": "git",
      "package.json": "json",
      "tsconfig.json": "typescript",
      "Cargo.toml": "rust",
      "go.mod": "go",
      "requirements.txt": "python",
      "Dockerfile": "file",
