{
  "id": "athas.html",
  "name": "HTML",
  "displayName": "HTML Language Support",
  "description": "HTML syntax highlighting",
  "version": "1.0.0",
  "publisher": "Athas",
  "license": "MIT",
  "category": "language",
  "bundled": true,
  "capabilities": {
    "type": "language",
    "languageId": "html",
    "fileExtensions": ["html", "htm", "xhtml"],
    "aliases": ["HTML", "html"],
    "grammar": {
      "wasmPath": "/tree-sitter/parsers/tree-sitter-html.wasm",
      "highlightQuery": "/tree-sitter/queries/html/highlights.scm",
      "scopeName": "text.html.basic"
    }
  }
}
