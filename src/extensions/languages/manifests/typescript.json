{
  "id": "language.typescript",
  "name": "TypeScript",
  "version": "1.0.0",
  "description": "TypeScript and JavaScript language support with syntax highlighting",
  "category": "language",
  "author": "Athas Team",
  "capabilities": {
    "languageProvider": {
      "id": "tsx",
      "extensions": ["ts", "tsx", "mts", "cts", "js", "jsx", "mjs", "cjs"],
      "aliases": ["typescript", "javascript", "ts", "js"],
      "wasmPath": "/extensions/tsx/parser.wasm",
      "highlightQuery": "/extensions/tsx/highlights.scm"
    }
  }
}
