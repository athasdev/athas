{
  "id": "language.javascript",
  "name": "JavaScript",
  "version": "1.0.0",
  "description": "JavaScript and JSX language support with syntax highlighting",
  "category": "language",
  "author": "Athas Team",
  "capabilities": {
    "languageProvider": {
      "id": "javascript",
      "extensions": ["js", "jsx", "mjs", "cjs"],
      "aliases": ["js", "jsx"],
      "wasmPath": "/tree-sitter/parsers/tree-sitter-javascript.wasm",
      "highlightQuery": "/tree-sitter/queries/javascript/highlights.scm"
    }
  }
}
